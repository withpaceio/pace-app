{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "skipLibCheck": true /* Skip type checking all .d.ts files. */,
    "types": ["geojson"],
    "baseUrl": "./src",
    "paths": {
      "@activity": ["activity"],
      "@api/*": ["api/*"],
      "@auth": ["auth"],
      "@components/*": ["components/*"],
      "@crypto": ["crypto"],
      "@models/*": ["models/*"],
      "@navigation/*": ["navigation/*"],
      "@subscription/*": ["subscription/*"],
      "@screens/*": ["screens/*"],
      "@tasks/*": ["tasks/*"],
      "@theme": ["theme"],
      "@translations/*": ["translations/*"],
      "@utils/*": ["utils/*"]
    }
  }
}
